
% if section == 'head':
	<link href="tiles.css" rel="stylesheet"/>
% elif section == 'content':

<div id="tiles">
	<div class="tile tile-right">
		<img src="cfsh.png" alt="night-distribution" width="30%">
		<div class="info">
			<div class="tile-header">
				<a class="tile-title" href="/cfsh"><h4>Cfsh</h4></a>
